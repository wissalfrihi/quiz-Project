<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quiz.ScoreController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  
  <Label text="Your Score: " />
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
         <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
              <Label fx:id="yourScore" alignment="CENTER" />
               <Label alignment="CENTER" text="/" />
               <Label fx:id="AllnbQuestions" alignment="CENTER" />
            </children>
         </VBox>
      </children>
   </AnchorPane>
</VBox>
